{
  "name": "apmd-www",
  "version": "0.1.1",
  "description": "appamada website",
  "main": "main.js",
  "scripts": {
    "build": "webpack",
    "dev": "webpack --mode development --watch",
    "image": "docker build -t apmd-www:$npm_package_version .",
    "image:do": "docker build -t registry.digitalocean.com/apmd/apmd-www:$npm_package_version .",
    "publish:do": "doctl registry login --expiry-seconds 1200 && docker push registry.digitalocean.com/apmd/apmd-www:$npm_package_version",
    "serve": "docker run --rm -p 80:80 --name apmd-www -v ./dist:/usr/local/apache2/htdocs apmd-www:$npm_package_version"
  },
  "keywords": [
    "apmd",
    "appamada",
    "www"
  ],
  "author": "mate@appamada.foundation",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "html-bundler-webpack-plugin": "^3.11.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.0.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
